generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Todo {
  id          Int      @id @default(autoincrement())
  parentId    Int?
  complete    Boolean? @default(false)
  userId      Int
  title       String
  description String?
  created     DateTime
  modified    DateTime
}
